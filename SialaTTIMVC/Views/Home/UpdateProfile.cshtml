@{
    Layout = "./_DashboardLayout.cshtml";
    ViewBag.Title = "Dashboard";
}
<section class="content">
    <div class="row">
        <div class="col-sm-12">
            <%-- <h3 class="row header smaller lighter blue">
                <span class="col-xs-6">Update Your Personal Information  </span>
                <!-- /.col -->
            </h3>--%>

            <div id="accordion" class="accordion-style1 panel-group">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                                <i class="ace-icon fa fa-angle-down bigger-110" data-icon-hide="ace-icon fa fa-angle-down" data-icon-show="ace-icon fa fa-angle-right"></i>
                                &nbsp;Personal Data
                            </a>
                        </h4>
                    </div>

                    <div class="panel-collapse collapse in" id="collapseOne">
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label">Student’s Name:</label>
                                        <div class="col-lg-12">
                                            <asp:TextBox ID="txtFNames" runat="server" type="text" Enabled="false" class="form-control"></asp:TextBox>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label label-sm">National ID/ Birth Certificate No:</label>
                                        <div class="col-lg-12">
                                            <asp:TextBox ID="txtIDno" runat="server" type="text" class="form-control"></asp:TextBox>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-12 control-label">Gender:</label>
                                        <div class="col-lg-12">
                                            <asp:DropDownList ID="ddlgender" runat="server" class="form-control">
                                                <asp:ListItem Value="0">Select</asp:ListItem>
                                                <asp:ListItem Value="1">Male</asp:ListItem>
                                                <asp:ListItem Value="2">Female</asp:ListItem>
                                            </asp:DropDownList>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-12 control-label">Date Of Birth:</label>
                                        <div class="col-lg-12">
                                            <asp:TextBox ID="txtDob" runat="server" type="text" Width="200px" class="form-control"></asp:TextBox>
                                            <script>
                                                $j('#Main1_txtDob').Zebra_DatePicker({
                                                    view: 'years',
                                                    direction: [1, true],
                                                });
                                            </script>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label">Mobile Phone No:</label>
                                        <div class="col-lg-12">
                                            <asp:TextBox ID="txtPhone" runat="server" type="text" class="form-control"></asp:TextBox>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-5">
                                    <div class="form-group">
                                        <label class="col-lg-12 control-label">Do you suffer from any Physical impairments?:</label>
                                        <div class="col-lg-12">
                                            <asp:DropDownList ID="ddlDis" runat="server" class="form-control" AutoPostBack="true">
                                                <asp:ListItem Value="0">No</asp:ListItem>
                                                <asp:ListItem Value="1">Yes</asp:ListItem>
                                            </asp:DropDownList>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label">If yes, please give details:</label>
                                        <div class="col-lg-12">
                                            <asp:TextBox ID="txtDisDetail" runat="server" type="text" class="form-control"></asp:TextBox>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-12 control-label">Ethnic Background:</label>
                                        <div class="col-lg-12">
                                            <asp:DropDownList ID="ddlEthnic" runat="server" class="form-control" AutoPostBack="true">
                                            </asp:DropDownList>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label">Nationality:</label>
                                        <div class="col-lg-12">
                                            <asp:DropDownList ID="ddlnationality" runat="server" class="form-control"></asp:DropDownList>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label">County:</label>
                                        <div class="col-lg-12">
                                            <asp:DropDownList ID="ddlCounty" runat="server" class="form-control">
                                            </asp:DropDownList>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-8">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label">Postal Address:</label>
                                        <div class="col-lg-4">
                                            Box:
                                            <asp:TextBox ID="txtBox" runat="server" type="text" class="form-control"></asp:TextBox>
                                        </div>
                                        <div class="col-lg-4">
                                            Codes:
                                            <asp:TextBox ID="txtCodes" runat="server" type="text" class="form-control"></asp:TextBox>
                                        </div>
                                        <div class="col-lg-4">
                                            Town:
                                            <asp:TextBox ID="txtTown" runat="server" type="text" class="form-control"></asp:TextBox>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label class="col-lg-10 control-label"></label>
                                        <br />
                                        <div class="col-lg-12">
                                            <label class="control-label">Your E-mail:</label>
                                            <asp:TextBox ID="txtEmail" runat="server" type="email" class="form-control"></asp:TextBox>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 text-center">
                    <asp:LinkButton ID="lbtnUpdate" class="btn btn-success" runat="server" OnClick="lbtnUpdate_Click" OnClientClick="return confirm('Are you sure you want to submit ?')"><i class="fa fa-check-square-o"></i> Submit Update</asp:LinkButton>
                </div>
            </div>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
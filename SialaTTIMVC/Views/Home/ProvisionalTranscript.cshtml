@{
    Layout = "./_DashboardLayout.cshtml";
    ViewBag.Title = "Transcript";
}
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-danger">
                <div class="box-header with-border">
                    <h3 class="box-title">Provisional Transcript</h3>
                </div>
                <div class="box-body">
                    <table>
                        <tr>
                            <td style="width: 286px">Select Period:</td>
                            <td class="auto-style1"></td>
                            <td></td>
                        </tr>
                        <tr>
                            <td style="width: 286px">
                                <asp:DropDownList ID="ddlPeriods" runat="server" AutoPostBack="False" CssClass="form-control"
                                                  Width="300px">
                                </asp:DropDownList>
                            </td>
                            <td class="auto-style1">
                                <strong>
                                    <asp:Button ID="Button1" runat="server" Text="View" CssClass="btn bg-olive btn-flat margin" OnClick="Button1_Click" />
                                    <span class="">*Select a Academic year to Diplay Results</span>
                                </strong>
                            </td>
                            <td></td>
                        </tr>
                        <tr>
                            <td style="width: 286px"></td>
                            <td class="auto-style1"></td>
                            <td></td>
                        </tr>
                    </table>
                    <p style="text-decoration: underline">
                        <%--<iframe runat="server" id="pdfLoans" src="" width="100%" height="500" />--%>
                    <p class="bg-danger">
                        <asp:Label ID="lblMsg" runat="server" Visible="false">&nbsp;Viewing results for this period has been disabled.</asp:Label>
                    </p>
                    <iframe runat="server" id="PdfExams" src="" width="100%" height="500" />
                </div>
            </div>
        </div>
    </div>
</section>
<!-- /.content -->